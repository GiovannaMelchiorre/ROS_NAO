<launch>
  
   <node name="camera_menager" pkg="astra_camera_pkg" type="camera_menager.py" />

   <node name="pc2topc" type="pc2topc" pkg="astra_camera_pkg" output="screen" />

   <node name="pointAnalysis" pkg="astra_camera_pkg" type="pointAnalysis.py" />



   <include file="$(find astra_launch)/launch/astra.launch">
   </include>


   	<arg name="gui" default="false"/>
	<param name="pport" value="$(arg pport)"/>
	<param name="pip" value="$(arg pip)"/>
	<param name="robot/name" value="NaoH25V50"/>
	<param name="robot_description" textfile="$(find nao_description)/urdf/naoV50_generated_urdf/nao.urdf"/>
	<param name="use_gui" value="$(arg gui)"/>


	<node name="nao_talker" pkg="nao_pkg" type="new_publisher.py"/>
	<node name="nao_movement" pkg="nao_pkg" type="movement_publisher.py"/>
	<node name="nao_movement_listener" pkg="nao_pkg" type="coordinates_subscriber.py"/>
	<node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher"/>

   <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch">
	<param name="port" value="9090"/>
   </include>
	
   <node name="file_server" pkg="file_server" type="file_server" output="screen"/>
</launch>


